{
  "name": "fc-manager-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --host",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "tsc": "tsc --noEmit --project tsconfig.app.json",
    "lint": "eslint .",
    "format": "prettier --write .",
    "preview": "vite preview",
    "prepare": "husky",
    "precommit": "bun run tsc && lint-staged",
    "db:login": "npx supabase login",
    "db:generate": "dotenv cross-var npx supabase gen types typescript --project-id %SUPABASE_PROJECT_ID% --schema public > src/database-generated.types.ts"
  },
  "dependencies": {
    "@mantine/charts": "^8.3.7",
    "@mantine/core": "^8.3.7",
    "@mantine/dates": "^8.3.7",
    "@mantine/form": "^8.3.7",
    "@mantine/hooks": "^8.3.7",
    "@mantine/modals": "^8.3.7",
    "@supabase/supabase-js": "^2.81.1",
    "@tanstack/react-router": "^1.135.2",
    "@tanstack/react-table": "^8.21.3",
    "@unocss/reset": "^66.5.6",
    "dayjs": "^1.11.19",
    "jotai": "^2.15.1",
    "lodash-es": "^4.17.21",
    "odometer_countup": "^1.0.4",
    "react": "^19.2.0",
    "react-country-flag": "^3.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.2.0",
    "react-usestateref": "^1.0.9",
    "recharts": "^3.4.1",
    "type-fest": "^5.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@iconify-json/mdi": "^1.2.3",
    "@tanstack/router-devtools": "^1.135.2",
    "@tanstack/router-plugin": "^1.135.2",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.2",
    "@unocss/preset-icons": "^66.5.6",
    "@unocss/preset-wind3": "66.5.6",
    "@vitejs/plugin-react": "^5.1.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "supabase": "^2.58.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "unocss": "^66.5.6",
    "vite": "^7.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
